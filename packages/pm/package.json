    "./model": "./src/model.ts",
    "./state": "./src/state.ts",
    "./transform": "./src/transform.ts",
    "./view": "./src/view.ts",
    "./view/style/prosemirror.css": "./src/view/style/prosemirror.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:tsup": "tsup",
    "build:tsc": "tsc -b tsconfig.json"
  },
  "dependencies": {
    "prosemirror-commands": "^1.6.2",
    "prosemirror-history": "^1.4.1",
