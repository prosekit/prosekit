{
  "name": "prosekit-website",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "The website for ProseKit",
  "scripts": {
    "astro": "astro",
    "build": "pnpm run build:typedoc && pnpm run build:astro",
    "build:typedoc": "bash build-typedoc.sh",
    "build:astro": "astro build",
    "build:css": "tsx build/gen-classes.ts",
    "build:if-not-exists": "if [ ! -d dist ]; then pnpm run build; fi",
    "preview": "pnpm run build:if-not-exists && astro preview",
    "dev": "astro dev",
    "lint": "pnpm run lint:svelte && pnpm run lint:vue && pnpm run lint:astro",
    "lint:svelte": "svelte-check --no-tsconfig",
    "lint:vue": "vue-tsc --build src/examples/vue/tsconfig.json",
    "lint:astro": "astro check",
    "test": "playwright test"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/preact": "^4.1.1",
    "@astrojs/react": "^4.3.1",
    "@astrojs/solid-js": "^5.1.1",
    "@astrojs/starlight": "^0.35.3",
    "@astrojs/svelte": "^7.1.1",
    "@astrojs/vue": "^5.1.1",
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@iconify-json/bxl": "^1.2.2",
    "@iconify-json/logos": "^1.2.9",
    "@iconify-json/lucide": "^1.2.68",
    "@iconify-json/tabler": "^1.2.23",
    "@manypkg/find-root": "^3.1.0",
    "@ocavue/utils": "^0.7.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/diffable-html": "^5.0.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "astro": "^5.13.8",
    "astro-minify-html-swc": "^0.1.7",
    "astro-rehype-relative-markdown-links": "^0.18.1",
    "astrobook": "^0.9.0",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "bits-ui": "^2.10.0",
    "clsx": "^2.1.1",
    "colorjs.io": "^0.5.2",
    "diffable-html": "^6.0.1",
    "fdir": "^6.5.0",
    "just-memoize": "^2.2.0",
    "lit": "^3.3.1",
    "loro-crdt": "^1.7.3",
    "loro-prosemirror": "^0.3.1",
    "magic-string": "^0.30.19",
    "preact": "^10.27.2",
    "prosekit": "workspace:*",
    "prosemirror-highlight": "^0.13.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rehype-parse": "^9.0.1",
    "rehype-remark": "^10.0.1",
    "remark-html": "^16.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "runed": "^0.31.1",
    "sharp": "^0.34.4",
    "solid-js": "^1.9.9",
    "starlight-theme-nova": "^0.9.5",
    "svelte": "^5.39.2",
    "svelte-check": "^4.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tinyexec": "^1.0.1",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.8",
    "typescript": "~5.9.2",
    "unified": "^11.0.5",
    "vite-plugin-wasm": "^3.5.0",
