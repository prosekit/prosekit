{
  "name": "prosemirror-math",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "description": "Math extensions for ProseMirror",
  "author": {
    "name": "ocavue",
    "email": "ocavue@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/prosekit/prosekit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prosekit/prosekit.git",
    "directory": "packages/prosemirror-math"
  },
  "bugs": {
    "url": "https://github.com/prosekit/prosekit/issues"
  },
  "keywords": [
    "ProseMirror"
  ],
  "sideEffects": false,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:tsc": "tsc -b tsconfig.json",
    "build:tsdown": "tsdown"
  },
  "dependencies": {
    "@ocavue/utils": "^1.5.0",
    "prosemirror-enter-rules": "workspace:^",
    "prosemirror-inputrules": "^1.5.1",
    "prosemirror-model": "^1.25.4",
    "prosemirror-state": "^1.4.4",
    "prosemirror-view": "^1.41.6"
  },
  "devDependencies": {
    "@mathjax/src": "^4.1.0",
    "@prosekit/config-vitest": "workspace:*",
    "@prosekit/core": "workspace:^",
    "@prosekit/pm": "workspace:^",
    "diffable-html-snapshot": "^0.2.0",
    "katex": "^0.16.28",
    "temml": "^0.13.1",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "vitest-browser-commands": "^0.2.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/prosemirror-math.d.ts",
        "default": "./dist/prosemirror-math.js"
      }
    },
    "dev": {},
    "main": "./dist/prosemirror-math.js",
    "module": "./dist/prosemirror-math.js",
    "types": "./dist/prosemirror-math.d.ts",
    "typesVersions": {
      "*": {
        ".": [
          "./dist/prosemirror-math.d.ts"
        ]
      }
    }
  },
  "dev": {
    "entry": {
      "prosemirror-math": "./src/index.ts"
    }
  }
}
