    "magic-string": "^0.30.21",
    "preact": "^10.27.2",
    "prosekit": "workspace:*",
    "prosekit-registry": "workspace:*",
    "prosemirror-highlight": "^0.13.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rehype-parse": "^9.0.1",
    "rehype-remark": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
