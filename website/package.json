{
  "name": "prosekit-website",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "The website for ProseKit",
  "scripts": {
    "astro": "astro",
    "gen": "pnpm run gen:css && pnpm run gen:registry",
    "gen:css": "tsx build/gen-classes.ts",
    "gen:registry": "tsx build/gen-registry.ts",
    "build": "pnpm run build:typedoc && pnpm run build:astro",
    "build:typedoc": "bash build-typedoc.sh",
    "build:astro": "astro build",
    "build:if-not-exists": "if [ ! -d dist ]; then pnpm run build; fi",
    "preview": "pnpm run build:if-not-exists && astro preview",
    "dev": "astro dev",
    "lint": "pnpm run lint:astro",
    "lint:astro": "astro check",
    "test": "vitest"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/preact": "^4.1.2",
    "@astrojs/react": "^4.4.1",
    "@astrojs/solid-js": "^5.1.2",
    "@astrojs/starlight": "^0.36.2",
    "@astrojs/svelte": "^7.2.1",
    "@astrojs/vue": "^5.1.2",
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@iconify-json/bxl": "^1.2.2",
    "@iconify-json/logos": "^1.2.10",
    "@iconify-json/lucide": "^1.2.72",
    "@iconify-json/tabler": "^1.2.23",
    "@manypkg/find-root": "^3.1.0",
    "@ocavue/utils": "^0.8.1",
    "@preact/preset-vite": "^2.10.2",
    "@prosekit/config-vitest": "workspace:*",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/vite": "^4.1.16",
    "@types/diffable-html": "^5.0.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser-playwright": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "astro": "^5.15.3",
    "astro-minify-html-swc": "^0.1.8",
    "astro-rehype-relative-markdown-links": "^0.18.1",
    "astrobook": "^0.10.0",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "bits-ui": "^2.14.2",
    "clsx": "^2.1.1",
