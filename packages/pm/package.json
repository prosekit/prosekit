{
  "name": "@prosekit/pm",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "author": {
    "name": "ocavue",
    "email": "ocavue@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/prosekit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ocavue/prosekit.git",
    "directory": "packages/pm"
